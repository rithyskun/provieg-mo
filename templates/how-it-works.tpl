<script type="text/javascript">
	$(function() {
		$('.collapse').on('show.bs.collapse', function() {
			var icon = $(this).parent().children().first().children().first();
			$(icon).removeClass('fa-file-text-o').addClass('fa-file-text');
		}).on('hide.bs.collapse', function() {
			var icon = $(this).parent().children().first().children().first();
			$(icon).removeClass('fa-file-text').addClass('fa-file-text-o');
		});
		if(jQuery.browser.mobile) { // mobile
			$('.table-controller').addClass('table-responsive');
			$('.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td').removeClass('table-desktop');
		}else { // desktop
			$('.table-controller').removeClass('table-responsive');
			$('.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td').addClass('table-desktop');
			// init tooltip
			$('.panel-heading > span').tooltip();
			// ellipse text
			$('.panel-heading > span').each(function() {
				var $title = $.trim($(this).text());
				$(this).attr('data-original-title', $title);
				$(this).text($.fn.text_ellipse($title, 35));
			});
		}
	});
	$.fn.extend({
		text_ellipse: function(text, length) {
			// strip all html tag before substr
			text = text.replace(/(<([^>]+)>)/ig, '');
			if(text.length > length) text = text.substr(0, length) + '...';
			return text;
		}
	});
</script>